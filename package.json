{
  "name": "gngm",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "test": "jest",
    "start": "ENVFILE=.env.development NODE_ENV=development react-native start",
    "start:clean": "ENVFILE=.env.development NODE_ENV=development react-native start --reset-cache",
    "build:prod:ios": "ENVFILE=.env.production NODE_ENV=production react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle",
    "build:prod:android": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "deploy:ios": "ENVFILE=.env.production xcodebuild -workspace ios/GNGM.xcworkspace -scheme GNGM -configuration Release -sdk iphoneos archive",
    "deploy:android": "cd android && ENVFILE=.env.production ./gradlew assembleRelease"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^5",
    "@mj-studio/react-native-naver-map": "1.5.10",
    "@react-native-google-signin/google-signin": "latest",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/stack": "^7.1.1",
    "@thewoowon/react-native-daum-postcode": "^1.1.3",
    "axios": "^1.7.9",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "react": "18.3.1",
    "react-native": "0.75.4",
    "react-native-animatable": "^1.4.0",
    "react-native-biometrics": "^3.0.1",
    "react-native-calendars": "^1.1308.1",
    "react-native-fs": "^2.20.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.23.0",
    "react-native-image-picker": "^8.0.0",
    "react-native-keychain": "^9.2.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-markdown-display": "^7.0.2",
    "react-native-permissions": "^5.2.5",
    "react-native-reanimated": "^3.16.7",
    "react-native-safe-area-context": "^5.2.0",
    "react-native-screens": "^4.6.0",
    "react-native-svg": "^15.11.1",
    "react-native-toast-message": "^2.2.1",
    "react-native-vision-camera": "^4.6.3",
    "react-native-webview": "^13.13.2"
